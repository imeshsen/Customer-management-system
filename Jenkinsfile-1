pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
          sh 'chmod 777 /var/lib/jenkins/workspace/sonar@tmp/durable-a2eb9a81/script.sh'
          sh './mvnw clean sonar:sonar'
        }
      }
    }
  }
}
